#include<iostream>
using namespace std;

int main()
{

char c;
cout<<"Please enter a letter\n";
cin>>c;


  if(c>=97 && c<= 122 )
      {
      cout<< char(c-32);
      }
  else if(c>=65 && c<=97)
      {
      cout<<char(c+32);
      }


else
  {
    cout<<"You entered a non-letter character please enter a letter"<<endl;
  }
return 0;




}
